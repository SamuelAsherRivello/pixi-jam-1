    "typescript": "^5.5.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "scripts": {
    "compile-client": "tsc -p src/scripts/client/tsconfig.json",
    "compile-server": "tsc -p src/scripts/server/tsconfig.json",
    "compile-shared": "tsc -p src/scripts/shared/tsconfig.json",
    "compile-all": "npm run compile-client && npm run compile-server && npm run compile-shared",
    "build-client": "webpack --mode production",
    "start-client": "webpack serve --mode development",
    "start-client-with-test": "concurrently --kill-others \"npm run watch:test\" \"webpack serve --mode development\"",
    "start-server": "nodemon --watch src/scripts/server --exec \"npx ts-node-esm src/scripts/server/server.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch:test": "onchange \"src/**/*\" \"tests/**/*\" -- npm run test",
    "format": "prettier --config prettier.config.cjs --write .",
    "lint": "eslint --config eslint.config.mjs",
    "lint:fix": "eslint --config eslint.config.mjs --fix",
    "analyze": "webpack --mode production --env analyze"
  },
  "devDependencies": {
    "@eslint/js": "^8.2.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "concurrently": "^7.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsdoc": "^39.0.6",
    "eslint-plugin-prettier": "^4.2.1",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "onchange": "^7.0.2",
    "prettier": "^2.8.8",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^7.16.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
